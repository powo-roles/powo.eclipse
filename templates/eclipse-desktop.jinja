#jinja2:trim_blocks: False
[Desktop Entry]
Name={{ _pec_item_desktop_name }}
Exec=env SWT_GTK3=0 {{ _pec_item_eclipse_bin }}{% if _pec_item_vm %} -vm {{ _pec_item_vm }}{% endif %}{% if _pec_item_vmargs %} -vmargs {{ _pec_item_vmargs }}{% endif %}
Type=Application
Description={{ _pec_item_description }}
Icon={{ _pec_item_eclipse_icon }}
